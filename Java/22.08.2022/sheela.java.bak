class sheela 
{
	static double wallet = 2000;
	public static void main(String[] args) 
	{
		System.out.println("Initial amount in wallet : "+wallet);
		double lipstick = 60;
		wallet = wallet - lipstick;
		System.out.println("Wallet balance after buying lipstick : "+wallet);
		double lakme = 450;
		wallet = wallet - lakme;
		System.out.println("Wallet balance after buying lakme : "+wallet);
		double cream = 300;
		wallet = wallet - cream;
		System.out.println("Balance after buying cream : "+wallet);
	}
}
